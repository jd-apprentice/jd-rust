[package]
name = "{{ package_name }}"
description = "{{ package_description }}"
version = "0.1.0"
edition = "2021"
documentation = ""
repository = ""
categories = ["command-line-utilities"]
keywords = ["cli"]

{% if license == "MIT" %}
license = "MIT"
{% endif %}
{% if license == "APACHE" %}
license = "Apache-2.0"
{% endif %}
{% if license == "all" %}
license = "MIT OR Apache-2.0"
{% endif %}

[dependencies]
{% if dotenv == true %}
dotenvy = "0.15.7"
{% endif %}

{% if sentry == true %}
sentry = "0.34.0"
tokio = { version = "1", features = ["full"] }
{% endif %}
